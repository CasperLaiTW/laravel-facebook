{
    "name": "vinkla/facebook",
    "description": "A Facebook bridge for Laravel",
    "keywords": [
        "facebook",
        "manager",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com",
            "homepage": "https://vinkla.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "facebook/graph-sdk": "^5.6",
        "graham-campbell/manager": "^4.2",
        "illuminate/contracts": "5.8.*",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.2",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^7.5 || ^8.0"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Facebook\\FacebookServiceProvider"
            ],
            "aliases": {
                "Facebook": "Vinkla\\Facebook\\Facades\\Facebook"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Facebook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Facebook\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
